{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2022"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "src/games/physics/VenturiEffectGame.ts",
    "src/games/physics/WaveInterferenceGame.ts",
    "src/games/physics/TransformerGame.ts",
    "src/games/physics/CapillaryActionGame.ts",
    "src/games/physics/CapacitiveTouchGame.ts",
    "src/games/physics/NewtonsThirdLawGame.ts",
    "src/games/physics/ElectricFieldGame.ts",
    "src/games/physics/NonNewtonianArmorGame.ts",
    "src/games/physics/CoulombsLawGame.ts",
    "src/games/physics/OrbitalMechanicsGame.ts",
    "src/games/physics/AngularMomentumTransferGame.ts",
    "src/games/physics/CentripetalForceGame.ts",
    "src/games/physics/MagneticFieldGame.ts",
    "src/games/physics/MagnusEffectGame.ts",
    "src/games/physics/MakeMicrophoneGame.ts",
    "src/games/physics/MomentOfInertiaGame.ts",
    "src/games/physics/PascalLawGame.ts",
    "src/games/physics/StaticKineticFrictionGame.ts",
    "src/games/physics/ThermalExpansionGame.ts",
    "src/games/physics/TidalForcesGame.ts",
    "src/games/physics/TidalLockingGame.ts",
    "src/games/physics/TotalInternalReflectionGame.ts",
    "src/games/physics/PrecessionNutationGame.ts",
    "src/games/physics/ProjectileIndependenceGame.ts",
    "src/games/physics/ShearThinningGame.ts"
  ]
}
